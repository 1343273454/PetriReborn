<root>
	<styles>
		<include src="file://{resources}/styles/custom_game/material_design.css" />	
		<include src="file://{resources}/styles/custom_game/game_setup.css" />
	</styles>
	<scripts>
		<include src="file://{resources}/scripts/custom_game/game_setup/game_setup_vote_utils.js" />
	</scripts>
	<Panel hittest="false" class="VoteContainer">
		<Panel id="VoteHeader" >
			<Label class="VoteQuestionLabel" text="#vote_game_length_q" />			
		</Panel>
		<Panel class="Paper" id="VoteVariants" param="game_length">
			<Button class="FlatButton" value="88">
				<Label class="FlatLabel" text="#vote_game_length_88" />
			</Button>
			<Button class="FlatButton" value="96">
				<Label class="FlatLabel" text="#vote_game_length_96" />
			</Button>
			<Button class="FlatButton" value="100" default="true">
				<Label class="FlatLabel" text="#vote_game_length_100" />
			</Button>
			<Button class="FlatButton" value="104">
				<Label class="FlatLabel" text="#vote_game_length_104" />
			</Button>
		</Panel>
	</Panel>
</root>

